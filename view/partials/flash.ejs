<% if (success && success.length > 0) { %>
          <div class="alert alert-success alert-dismissible fade show mt-3 mx-3" role="alert">
            <%= success[0] %>
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
          </div>
        <% } %>

        <% if (successEdit && successEdit.length > 0) { %>
          <div class="alert alert-info alert-dismissible fade show mt-3 mx-3" role="alert">
            <%= successEdit[0] %>
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
          </div>
        <% } %>

        <% if (successDelete && successDelete.length > 0) { %>
          <div class="alert alert-warning alert-dismissible fade show mt-3 mx-3" role="alert">
            <%= successDelete[0] %>
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
          </div>
        <% } %>
        <% if (error && error.length > 0) { %>
          <div class="alert alert-danger alert-dismissible fade show mt-3 mx-3" role="alert">
            <%= error[0] %>
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
          </div>
        <% } %>

        <% if (successUser && successUser.length > 0) { %>
  <div class="alert alert-success alert-dismissible fade show mt-3" role="alert">
    <%= successUser[0] %>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
  </div>
<% } %>

<% if (errorUserDuplicate && errorUserDuplicate.length > 0) { %>
  <div class="alert alert-danger alert-dismissible fade show mt-3">
    <%= errorUserDuplicate[0] %>
  </div>
<% } %>

<% if (loginError && loginError.length > 0) { %>
  <div class="alert alert-danger alert-dismissible fade show mt-3" role="alert">
    <%= loginError[0] %>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
  </div>
<% } %>

<% if (loginSuccess && loginSuccess.length > 0) { %>
  <div class="alert alert-success alert-dismissible fade show mt-3" role="alert">
    <%= loginSuccess[0] %>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
  </div>
<% } %>


<% if (errorNAuthenticated && errorNAuthenticated.length > 0) { %>
  <div class="alert alert-success alert-dismissible fade show mt-3" role="alert">
    <%= errorNAuthenticated[0] %>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
  </div>
<% } %>